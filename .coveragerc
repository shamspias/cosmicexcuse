[run]
source = cosmicexcuse
branch = True
omit =
    */tests/*
    */test_*.py
    */__init__.py
    */conftest.py
    setup.py
    cosmicexcuse/__version__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov

[xml]
output = coverage.xml
